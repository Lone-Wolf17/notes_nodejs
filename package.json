{
  "type": "module",
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "REQUEST_LOG_FILE=log.txt REQUEST_LOG_FORMAT=common cross-env DEBUG=notes:* node ./app.js",
    "dl-minty": "mkdir -p minty && npm run dl-minty-css && npm run dl-minty-min-css",
    "dl-minty-css": "wget https://bootswatch.com/5/minty/bootstrap.css -O minty/bootstrap.css",
    "dl-minty-min-css": "wget https://bootswatch.com/5/minty/bootstrap.min.css -O minty/bootstrap.min.css",
    "fs-start": "cross-env DEBUG=notes:* NOTES_MODEL=fs PORT=3000 node ./app.js",
    "fs-server1": "cross-env NOTES_MODEL=fs PORT=3001 node ./app.js",
    "fs-server2": "cross-env NOTES_MODEL=fs PORT=3002 node ./app.js",
    "level-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=level node ./app.js",
    "sqlite3-setup": "sqlite3 chap07.sqlite3 --init models/schema-sqlite3.sql",
    "sqlite3-start": "cross-env SQLITE_FILE=chap07.sqlite3 DEBUG=notes:* NOTES_MODEL=sqlite3 node ./app.js",
    "sqlite3-server1": "cross-env SQLITE_FILE=chap07.sqlite3 DEBUG=notes:* NOTES_MODEL=sqlite3 PORT=3001 node ./app.js",
    "sqlite3-server2": "cross-env SQLITE_FILE=chap07.sqlite3 DEBUG=notes:* NOTES_MODEL=sqlite3 PORT=3002 node ./app.js",
    "sequelize-start": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize node ./app.js",
    "sequelize-server1": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3001 node ./app.js",
    "sequelize-server2": "cross-env DEBUG=notes:* SEQUELIZE_CONNECT=models/sequelize-sqlite.yaml NOTES_MODEL=sequelize PORT=3002 node ./app.js",
    "mongodb-start": "cross-env DEBUG=notes:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb node ./app.js",
    "mongodb-server1": "cross-env DEBUG=notes:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb PORT=3001 node ./app.js",
    "mongodb-server2": "cross-env DEBUG=notes:* MONGO_URL=mongodb://localhost/ MONGO_DBNAME=chap07 NOTES_MODEL=mongodb PORT=3002 node ./app.js"

  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.5",
    "bootstrap": "5.0.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "feather-icons": "^4.28.0",
    "fs-extra": "^10.0.0",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "jquery": "3.5.x",
    "level": "^6.0.1",
    "mongodb": "^4.0.1",
    "morgan": "~1.9.1",
    "popper.js": "1.16.x",
    "rotating-file-stream": "^2.1.5",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  }
}
