{
  "type": "module",
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "REQUEST_LOG_FILE=log.txt REQUEST_LOG_FORMAT=common cross-env DEBUG=notes:* node ./app.js",
    "dl-minty": "mkdir -p minty && npm run dl-minty-css && npm run dl-minty-min-css",
    "dl-minty-css": "wget https://bootswatch.com/5/minty/bootstrap.css -O minty/bootstrap.css",
    "dl-minty-min-css": "wget https://bootswatch.com/5/minty/bootstrap.min.css -O minty/bootstrap.min.css",
    "fs-start": "cross-env DEBUG=notes:* NOTES_MODEL=fs PORT=3000 node ./app.js",
    "fs-server1": "cross-env NOTES_MODEL=fs PORT=3001 node ./app.js",
    "fs-server2": "cross-env NOTES_MODEL=fs PORT=3002 node ./app.js"

  },
  "dependencies": {
    "bootstrap": "5.0.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "feather-icons": "^4.28.0",
    "fs-extra": "^10.0.0",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "jquery": "3.5.x",
    "morgan": "~1.9.1",
    "popper.js": "1.16.x",
    "rotating-file-stream": "^2.1.5"
  }
}
